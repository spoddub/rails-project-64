{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:css": "sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --load-path=node_modules --quiet"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "esbuild": "^0.25.10",
    "sass-embedded": "^1.92.1"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.16",
    "bootstrap": "^5.3.8"
  }
}
